pipeline{
    agent any
    stages{
        stage("Git CHeckout Stage"){
            steps{
            git credentialsId: 'githubcredentials', url: 'https://github.com/rajatgit123/dockerwebapp.git'
        }
        }
        stage("sonarqube analysis"){
            steps{
                nodejs(nodeJSInstallationName: 'nodejs'){
                    withSonarQubeEnv('sonar'){
                        sh "npm install"
                        sh "npm install sonar-scanner"
                        sh "npm run sonar"
    }
}
}
}
}
}
